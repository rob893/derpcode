{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "AllowedOrigins": ["http://localhost:5173", "https://localhost:5173"],
    "ExposedHeaders": [
      "X-Token-Expired",
      "X-Correlation-Id",
      "X-Rate-Limit-Limit",
      "X-Rate-Limit-Remaining",
      "X-Rate-Limit-Reset"
    ]
  },
  "Authentication": {
    "TokenAudience": "derpcode",
    "TokenIssuer": "derpcode",
    "TokenExpirationTimeInMinutes": 60,
    "RefreshTokenExpirationTimeInMinutes": 43200,
    "GoogleOAuthAudiences": [],
    "CookieDomain": ".derpcode.dev"
  },
  "KeyVaultUrl": "https://rherber-kv-ue-d.vault.azure.net",
  "Swagger": {
    "Enabled": true,
    "SupportedApiVersions": ["v1", "v2"],
    "AuthSettings": {
      "RequireAuth": false
    }
  }
}
