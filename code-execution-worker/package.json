{
  "name": "code-execution-worker",
  "version": "1.0.0",
  "description": "Executes code",
  "license": "MIT",
  "author": "Robert Herber",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "prettier": "prettier --write **/*.{ts,js,css,html,json} !**/node_modules/** !**/dist/** !**/coverage/** !**/compiled/**",
    "docker-build-csharp": "cd docker/csharp && docker build -t code-executor-csharp .",
    "docker-build-js": "cd docker/javascript && docker build -t code-executor-js .",
    "docker-build-ts": "cd docker/typescript && docker build -t code-executor-ts ."
  },
  "dependencies": {
    "@types/cors": "^2.8.18",
    "cors": "^2.8.5",
    "dockerode": "^4.0.6",
    "express": "^5.1.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/dockerode": "^3.3.38",
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.17",
    "eslint": "^9.26.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0"
  }
}
